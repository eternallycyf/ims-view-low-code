{
  "name": "ims-view-low-code",
  "version": "1.1.0",
  "description": "a ims-view low code editor",
  "keywords": [
    "npm",
    "template"
  ],
  "homepage": "https://github.com/eternallycyf/ims-view-low-code",
  "bugs": {
    "url": "https://github.com/eternallycyf/ims-view-low-code/issues/new"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eternallycyf/ims-view-low-code.git"
  },
  "license": "MIT",
  "author": "eternallycyf(969475322@qq.com)",
  "sideEffects": [
    "**/*.less"
  ],
  "main": "lib/index.js",
  "unpkg": "dist/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "changelog": "conventional-changelog -n node_modules/conventional-changelog-gitmoji-config/lib/index.js -i CHANGELOG.md -s -r 0",
    "check-error": "npm run lint && npm run test && npm run test:coverage && npm run doctor",
    "ci": "npm run lint && npm run type-check && npm run doctor && npm run build",
    "clean": "rm -rf es lib dist coverage .dumi/tmp .eslintcache",
    "dev": "npm run tailwind:watch && dumi dev",
    "docs:build": "npm run tailwind && dumi build",
    "docs:preview": "yarn docs:build && cd dist && npx serve",
    "doctor": "father doctor",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && npm run setup",
    "prepublishOnly": "npm run doctor && npm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "release": "npm run ci && semantic-release",
    "semantic-release": "semantic-release",
    "setup": "dumi setup",
    "start": "dumi dev",
    "tailwind": "npx @tailwindcss/cli -i ./src/global.css -o ./src/output.css",
    "tailwind:watch": "npx @tailwindcss/cli -i ./src/global.css -o ./src/output.css --watch",
    "test": "vitest --passWithNoTests",
    "test:coverage": "vitest run --coverage --passWithNoTests",
    "test:update": "vitest -u",
    "type-check": "tsc -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@ims-view/chart": "^1.8.1",
    "@ims-view/hooks": "^1.18.1",
    "@ims-view/utils": "^1.17.1",
    "@monaco-editor/react": "^4.6.0",
    "allotment": "^1.20.2",
    "axios": "^1.7.4",
    "dayjs": "^1.11.13",
    "ims-view-pc": "^1.77.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "style-to-object": "^1.0.6",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@commitlint/cli": "^17",
    "@tailwindcss/cli": "^4.1.12",
    "@testing-library/react": "^14",
    "@types/lodash": "^4.14.199",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@umijs/lint": "^4",
    "@umijs/plugins": "^4.4.12",
    "@vitest/coverage-v8": "^1.x",
    "antd": "latest",
    "autoprefixer": "^10.4.21",
    "babel-plugin-add-module-exports": "^1",
    "babel-plugin-antd-style": "^1.0.4",
    "commitlint": "^17",
    "commitlint-config-gitmoji": "^2",
    "concurrently": "^7",
    "conventional-changelog-gitmoji-config": "^1",
    "cross-env": "^7",
    "dumi": "latest",
    "dumi-theme-antd-style": "latest",
    "eslint": "^8",
    "father": "^4",
    "husky": "^8",
    "ims-template-config": "latest",
    "jsdom": "^22",
    "lint-staged": "^13",
    "lodash": "^4.17.21",
    "postcss": "^8.5.6",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^20",
    "semantic-release-config-gitmoji": "^1",
    "stylelint": "^15",
    "tailwindcss": "^4.1.12",
    "typescript": "^5",
    "vitest": "^1.x"
  },
  "peerDependencies": {
    "@ant-design/icons": "^5.2.6",
    "antd": "^5.9.4",
    "lodash": "^4.17.21"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}